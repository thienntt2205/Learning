<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CcdCommonDBDAOCheckCrrCodeRSQL">
			<desc><![CDATA[MDM_CARRIER 테이블에 crrCd에 해당하는 정보가 있는지 유무를 리턴한다.]]></desc>
			<sql><![CDATA[
SELECT CRR_CD 
FROM MDM_CARRIER
WHERE 1=1 
AND CRR_CD = @[crr_cd]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
