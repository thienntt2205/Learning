<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JooCarrierMgmtDBDAOSearchCustCdRSQL">
			<desc><![CDATA[search customer code]]></desc>
			<sql><![CDATA[
SELECT A.CUST_CNT_CD, A.CUST_SEQ 
FROM MDM_CUSTOMER A
WHERE 1 = 1
AND A.CUST_CNT_CD = @[cust_cnt_cd]
AND A.CUST_SEQ = @[cust_seq]
			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
