<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CcdCommonDBDAOCheckLocCodeRSQL">
			<desc><![CDATA[MDM_LOCATION 테이블에 locCd 에 
해당하는 정보가 있는지 유무를 리턴한다.]]></desc>
			<sql><![CDATA[
SELECT LOC_CD 
FROM MDM_LOCATION 
WHERE 1=1 
AND LOC_CD = @[loc_cd]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
