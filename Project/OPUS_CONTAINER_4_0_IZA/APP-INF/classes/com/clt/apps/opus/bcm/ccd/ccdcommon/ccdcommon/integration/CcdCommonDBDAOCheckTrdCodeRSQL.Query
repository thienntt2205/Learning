<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CcdCommonDBDAOCheckTrdCodeRSQL">
			<desc><![CDATA[MDM_TRADE 테이블에 trdCd 에 
해당하는 정보가 있는지 유무를 리턴한다.]]></desc>
			<sql><![CDATA[
SELECT TRD_CD 
FROM MDM_TRADE
WHERE 1=1 
AND TRD_CD = @[trd_cd]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
